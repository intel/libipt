; Test that a PIP is processed while tracing is disabled.
;
; Variant: not enabled at syncpoint.
;

org 0x100000
bits 64

; @pt p1: psb()
; @pt p2: mode.exec(64bit)
; @pt p3: psbend()

; @pt p4: pip(0xa00)
; @pt p5: tip.pge(3: %l1)
l1:     nop

l2:     nop
l3:     hlt
