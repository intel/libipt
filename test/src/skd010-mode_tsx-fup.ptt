; SKD010: Intel(R) PT FUP May be Dropped After OVF.
;
;         Some Intel PT (Intel Processor Trace) OVF (Overflow) packets may not
;         be followed by a FUP (Flow Update Packet) or TIP.PGE (Target IP
;         Packet, Packet Generation Enable).
;
;   cpu 6/78
;   cpu 6/94
;   cpu 6/142
;   cpu 6/158
;   cpu 6/165
;   cpu 6/166
;
; Variant: Missing FUP, sync at MODE.TSX + FUP.
;

org 0x1000
bits 64

; @pt p0: psb()
; @pt p1: mode.exec(64bit)
; @pt p2: psbend()

; @pt p3: ovf()
;         fup missing

; @pt p4: mode.tsx(begin)
; @pt p5: fup(3: %l0)
l0: nop

; @pt p6: fup(1: %l1)
; @pt p7: tip.pgd(0: %l2)
l1: nop
l2: hlt


; @pt .exp(ptdump)
;%0p0  psb
;%0p1  mode.exec  cs.l
;%0p2  psbend
;%0p3  ovf
;%0p4  mode.tsx   intx
;%0p5  fup        3: %?l0
;%0p6  fup        1: %?l1.2
;%0p7  tip.pgd    0: %?l2.0


; @pt .exp(ptxed)
;[overflow]
;[begin transaction]
;? %0l0
;[disabled]
