; Test that TIP.PGD is applied to the next branch (jump in this case) that
; would normally generate a TIP packet.
;

org 0x100000
bits 64

; @pt p1: psb()
; @pt p2: fup(3: %l1)
; @pt p3: mode.exec(64bit)
; @pt p4: psbend()
l1:     jmp l3
l2:     hlt
l3:     jmp rax
l4:     hlt

; @pt p5: tip.pgd(3: %l5)
l5:     nop


